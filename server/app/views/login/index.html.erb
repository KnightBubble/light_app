<div class="container">
  <form class="form-signin" action="<%= login_check_url %>" method="post">
    <h2 class="form-signin-heading">系统登录</h2>
    <p><%= notice %></p>
    <input type="text" name="user_name" class="input-block-level" placeholder="请输入用户名">
    <input type="password" name="user_pass" class="input-block-level" placeholder="请输入密码">
    <button class="btn btn-primary" type="submit">登录</button>
  </form>
</div>